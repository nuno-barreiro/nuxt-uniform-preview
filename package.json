{
  "name": "nuxt-uniform-preview",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "run-p start:func start:nuxt",
    "start:func": "cd api && yarn start",
    "start:nuxt": "nuxt generate && nuxt start",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:prettier",
    "lintfix": "prettier --write --list-different .",
    "postinstall": "cd api && yarn install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@uniformdev/canvas": "^16.0.0",
    "@uniformdev/canvas-nuxt": "^14.2.0",
    "core-js": "^3.19.3",
    "isomorphic-unfetch": "^3.1.0",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  }
}
